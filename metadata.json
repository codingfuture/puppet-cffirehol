{
  "name": "codingfuture-cffirehol",
  "version": "0.11.5",
  "author": "Andrey Galkin",
  "summary": "Firewall provider for cfnetwork based on FireHOL generator",
  "license": "Apache-2.0",
  "source": "https://github.com/codingfuture/puppet-cffirehol",
  "project_page": "https://github.com/codingfuture/puppet-cffirehol",
  "issues_url": "https://github.com/codingfuture/puppet-cffirehol/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "16.04",
        "16.10",
        "17.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=4.8.0 <6.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-apt",
      "version_requirement": ">= 4.1.0 <5.0.0"
    },
    {
      "name": "codingfuture-cfnetwork",
      "version_requirement": ">= 0.11.0 < 0.12.0"
    }
  ],
  "data_provider": "hiera"
}
