{
  "name": "codingfuture-cffirehol",
  "version": "0.9.5",
  "author": "Andrey Galkin",
  "summary": "Firewall provider for cfnetwork based on FireHOL generator",
  "license": "Apache-2.0",
  "source": "https://github.com/codingfuture/puppet-cffirehol",
  "project_page": "https://github.com/codingfuture/puppet-cffirehol",
  "issues_url": "https://github.com/codingfuture/puppet-cffirehol/issues",
  "operatingsystem_support": [
    {
